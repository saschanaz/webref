{
  "spec": {
    "title": "Scroll-linked Animations",
    "url": "https://drafts.csswg.org/scroll-animations-1/"
  },
  "properties": {},
  "descriptors": {
    "source": {
      "name": "source",
      "for": "@scroll-timeline",
      "value": "selector( <id-selector> ) | auto | none",
      "initial": "auto"
    },
    "orientation": {
      "name": "orientation",
      "for": "@scroll-timeline",
      "value": "auto | block | inline | horizontal | vertical",
      "initial": "auto"
    },
    "start": {
      "name": "start",
      "for": "@scroll-timeline",
      "value": "<scroll-timeline-offset>",
      "initial": "auto"
    },
    "end": {
      "name": "end",
      "for": "@scroll-timeline",
      "value": "<scroll-timeline-offset>",
      "initial": "auto"
    },
    "time-range": {
      "name": "time-range",
      "for": "@scroll-timeline",
      "value": "auto | <time>",
      "initial": "auto"
    }
  },
  "valuespaces": {
    "<scroll-timeline-offset>": {
      "value": "auto | <length-percentage> | <element-offset>"
    },
    "<element-offset>": {
      "value": "selector( <id-selector> ) [<element-offset-edge> || <number>]?"
    },
    "<element-offset-edge>": {
      "value": "start | end"
    }
  }
}
